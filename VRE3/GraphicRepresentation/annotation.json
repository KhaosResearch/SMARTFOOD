{
  "type": "DataSink",
  "name": "graphic-representation",
  "label": "Graphic Representation",
  "description": "Plot execution info generated by smartfood epanet optimization component.",
  "license": "MIT",
  "version": "1.0.0",
  "dockerImage": "192.168.219.5:8098/smartfood/vre3/graphicrepresentation:1.0.0",
  "tags": ["smartfood", "vre3", "graphic representation"],
  "parameters": [
    {
      "name": "--fitness-evolution-file",
      "label": "Fitness Evolution File",
      "description": "Fitness evolution file",
      "defaultValue": "{{.inputs.fitness_evolution.path}}",
      "readOnly": true,
      "type": "string"
    },
    {
      "name": "--fun-file",
      "label": "Function File",
      "description": "File with function values",
      "defaultValue": "{{.inputs.fun_file.path}}",
      "readOnly": true,
      "type": "string"
    },
    {
      "name": "--output-folder",
      "label": "Output directory",
      "description": "Path to the output directory",
      "defaultValue": "/mnt/shared/",
      "readOnly": true,
      "type": "string"
    }
  ],
  "inputs": [
    {
      "name": "fitness_evolution",
      "label": "Fitness Evolution",
      "path": "/mnt/shared/fitness_evolution.txt",
      "type": "text"
    },
    {
      "name": "fun_file",
      "label": "Fun File",
      "path": "/mnt/shared/fun_file.csv",
      "type": "tabulardataset"
    }
  ],
  "outputs": [
    {
      "name": "output",
      "label": "Output file",
      "path": "/mnt/shared/output.zip",
      "type": "zip"
    }
  ],
  "mainScriptPath": "main.py",
  "testPath": "test_main.py",
  "dependencies": [
    {
      "name": "python",
      "version": "3.11.5",
      "license": "PSF",
      "url": "https://www.python.org/"
    }
  ],
  "resources": {
    "cores": 1,
    "memory": 128,
    "gpuNeeded": false,
    "gpuMemory": null,
    "estimatedTimeInMin": 4
  },
  "publicationDate": "2023-11-30T10:00:00.000Z",
  "author": {
    "email": "khaos.research@gmail.com",
    "affiliation": "Khaos Research Group"
  },
  "contributor": [
    {
      "email": "adrianseor.99@uma.es",
      "affiliation": "Khaos Research Group"
    }
  ]
}